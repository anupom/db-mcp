# Cube API Configuration
CUBE_API_URL=http://localhost:4000/cubejs-api/v1

# JWT Configuration (min 32 characters)
CUBE_JWT_SECRET=your-very-long-secret-key-here-at-least-32-chars
CUBE_JWT_EXPIRES_IN=1h

# Query Limits
MAX_LIMIT=1000

# Default segments (comma-separated)
DEFAULT_SEGMENTS=

# Denied members (comma-separated)
DENY_MEMBERS=

# Debug options
RETURN_SQL=false
LOG_LEVEL=info

# Agent catalog path
AGENT_CATALOG_PATH=agent_catalog.yaml
